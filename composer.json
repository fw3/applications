{
	"name" : "fw3/applications",
	"description" : "Flywheel3 framework: applications library",
	"type" : "library",
	"homepage" : "https://ickx.jp",
	"license" : "MIT",
	"minimum-stability" : "dev",
	"authors" : [{
			"name" : "akira wakaba",
			"homepage" : "https://ickx.jp",
			"role" : "lead developer"
		}
	],
	"config" : {
		"autoloader-suffix" : "IckxFw3applications"
	},
